<div class="row">
  <div class="col-xs-8">
    <h2>Free Busy Information</h2>
  </div>
</div>

<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>Status</th>
    <th>Start Time</th>
    <th>End Time</th>
  </tr>
  </thead>

  <tbody>
  <% @free_busy.each do |state| %>
      <tr>
        <td><%= state.free_busy_status %></td>
        <td><%= state.start.to_time %></td>
        <td><%= state.end.to_time %></td>
      </tr>
  <% end %>
  </tbody>
</table>

